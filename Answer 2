package com.company;

import java.util.ArrayList;
import java.util.Collections;
import java.util.HashMap;
import java.util.List;

//Answer2
public class Answer2 {
    public static void main(String[] args) {

        //creating hashmap
        HashMap<String, List> phoneBrand =new HashMap<>();

        //for samsung
        List<String> samsung=new ArrayList<>();
        samsung.add("Galaxy");
        Collections.sort(samsung);
        phoneBrand.put("Samsung",samsung);

        //for google
        List<String> google=new ArrayList<>();
        google.add("Nexus");
        google.add("Pixel");
        Collections.sort(google);
        phoneBrand.put("Google",google);

        //for realme
        List<String> realme=new ArrayList<>();
        realme.add("X");
        realme.add("3 Pro");
        Collections.sort(realme);
        phoneBrand.put("Realme",realme);

        //for oneplus
        List<String> oneplus=new ArrayList<>();
        oneplus.add("One");
        oneplus.add("5");
        oneplus.add("7Pro");
        oneplus.add("One");
        Collections.sort(oneplus);
        phoneBrand.put("Oneplus",oneplus);

        //System.out.println(phoneBrand);
        System.out.println("For google = " + phoneBrand.get("Google"));

    }
}
